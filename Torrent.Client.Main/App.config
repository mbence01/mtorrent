<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
	
	<appSettings>
		<add key="DefaultTorrentEncoding" value="UTF-8" />
		
		<!-- Key names which will be in the parsed dictionary from torrent file -->
		<add key="PropAnnounce" value="announce" />
		<add key="PropCreatedBy" value="created by" />
		<add key="PropCreationDate" value="creation date" />
		<add key="PropEncoding" value="encoding" />
		<add key="PropInformation" value="info" />
		
		<!-- Key names which will be in the parsed dictionary from torrent info -->
		<add key="PropInfoFiles" value="files" />
		<add key="PropInfoName" value="name" />
		<add key="PropInfoLength" value="length" />
		<add key="PropInfoPieceLength" value="piece length" />
		<add key="PropInfoPieces" value="pieces" />
		<add key="PropInfoPrivate" value="private" />
		<add key="PropInfoSource" value="source" />

		<!-- Key names which will be in the parsed dictionary from torrent piece -->
		<add key="PropPieceLength" value="length" />
		<add key="PropPiecePath" value="path" />
	</appSettings>
	
    <runtime>
